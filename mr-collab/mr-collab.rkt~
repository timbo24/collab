#lang racket/base
(require drracket/tool
         racket/class
         racket/gui/base
         racket/unit
         mrlib/switchable-button)
(provide tool@)
 

(define tool@
  (unit
    (import drracket:tool^)
    (export drracket:tool-exports^)
 
    (define (phase1) (void))
    (define (phase2) (void))

    (define txt (drracket:get/extend:get-definitions-text))

    (send txt delete 0 1000000 #t)

    (send txt insert "butts ahoy!" 0 0)))